# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Security
SECRET_KEY=${GENERATED_SECRET_KEY}  # Will be automatically generated by Render

# Proxy Configuration
PROXY_ENABLED=false
PROXY_LIST_FILE=proxies.txt

# Browser Configuration
BROWSER_HEADLESS=true
USER_AGENTS_FILE=user_agents.txt

# Email Services Configuration
GMAIL_APP_PASSWORD=your-app-password
OUTLOOK_APP_PASSWORD=your-app-password

# Database Configuration
DATABASE_URL=sqlite:///./emailbot.db

# Logging
LOG_LEVEL=INFO
LOG_FILE=emailbot.log 